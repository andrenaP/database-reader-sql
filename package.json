  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "gh-pages": "^6.1.1",
    "react-app-rewired": "^2.2.1",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "github.com/andrenaP",
    "productName": "DatabaseReader",
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "public"
    }
  }
